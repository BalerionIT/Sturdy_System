        "#arpu per country\n",
        "\n",
        "arpu_per_country <- df %>%\n",
        "  group_by(country, customer_id) %>%\n",
        "  summarise(total_revenue = sum(total_price)) %>%\n",
        "  summarise(arpu_value = mean(total_revenue)) %>% arrange(desc(arpu_value))\n",
        "\n",
        "global_mean_arpu <- mean(arpu_per_country$arpu_value)\n",
        "global_sd_arpu <- sd(arpu_per_country$arpu_value)\n",
        "\n",
        "arpu_per_country <- arpu_per_country %>%\n",
        "  mutate(arpu_zscore = (arpu_value - global_mean_arpu) / global_sd_arpu) %>%\n",
        "  arrange(desc(arpu_zscore))\n",
        "\n",
        "head(arpu_per_country)"
