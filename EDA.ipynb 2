      "source": [
        "France <- df %>% filter(country == 'France') %>%\n",
        "  count(category, sort = TRUE) %>%\n",
        "  top_n(10) %>% ggplot(aes(x = reorder(category, n), y = n, fill = category)) +\n",
        "    geom_col()+\n",
        "    labs(title = 'Top product France', x ='Category', y = 'Count')+\n",
        "      theme(axis.text.x = element_text(angle = 45, hjust =1))\n",
        "\n",
        "EIRE <-  df %>% filter(country == 'EIRE') %>%\n",
        "  count(category, sort = TRUE) %>%\n",
        "  top_n(10) %>% ggplot(aes(x = reorder(category, n), y = n, fill = category)) +\n",
