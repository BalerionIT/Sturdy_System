    {
      "cell_type": "code",
      "source": [
        "top_countries <- df %>%\n",
        "  group_by(country) %>%\n",
        "  summarise(order_count = n_distinct(invoice_no)) %>%\n",
        "  top_n(10, order_count) %>%\n",
        "  pull(country)\n",
        "ggplot(df %>%\n",
        "         group_by(country, invoice_no) %>%\n",
        "         summarise(order_value = sum(total_price), .groups = \"drop\") %>%\n",
        "         filter(country %in% top_countries),  # Filter top 10 countries by order count\n",
        "       aes(x = country, y = order_value)) +\n",
        "  geom_boxplot(aes(fill = country), outlier.colour = \"red\", outlier.size = 3) +\n",
        "  labs(title = \"Order Value for the Top 10 Countries by Number of Orders\", x = \"Countries\", y = \"Order Value\") +\n",
        "  theme(axis.text.x = element_text(angle = 45, hjust = 1))"
