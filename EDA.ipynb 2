        "df %>%\n",
        "  mutate(day = wday(date, label = TRUE)) %>%\n",
        "  group_by(day, hour) %>%\n",
        "  summarise(revenue = sum(total_price)) %>%\n",
        "  ggplot(aes(x = hour, y = day, fill = revenue)) +\n",
        "  geom_tile() +\n",
        "  scale_fill_viridis_c() +\n",
        "  labs(title = \"Heatmap of Revenue by Hour and Day\", x = \"Hour\", y = \"Day of Week\") +\n",
