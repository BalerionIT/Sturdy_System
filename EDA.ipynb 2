        "# Drop rows with missing CustomerID\n",
        "df <- df %>% drop_na(customer_id)\n",
        "\n",
        "# Convert date/time and compute total revenue\n",
        "df <- df %>%\n",
        "  mutate(\n",
        "    invoice_date = as_date(invoice_date),\n",
        "    date = as_date(date),\n",
        "    hour = hour(hms(hour)),\n",
        "    total_price= quantity * unit_price\n",
        "  )\n"
      ],
