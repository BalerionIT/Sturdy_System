      "cell_type": "code",
      "source": [
        "#radar chart of top and worst perfromers.\n",
        "top_4 <- metrics_standardized %>%\n",
        "  arrange(desc(performance_index)) %>%\n",
        "  slice_head(n = 4)\n",
        "worst_4 <- metrics_standardized %>% arrange(performance_index) %>% slice_head(n=4)\n",
        "perf <- rbind(top_4, worst_4)\n",
        "\n",
        "ggplot(perf, aes(x = reorder(country, performance_index), y=performance_index)) + geom_bar(stat ='identity', fill = 'steelblue')+\n",
        "  coord_flip()+theme_minimal()+labs(x = 'performance index', y = 'countries')\n"
