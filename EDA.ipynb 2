    {
      "cell_type": "code",
      "source": [
        "#AOV per country\n",
        "\n",
        "aov_per_country <- df %>%\n",
        "  group_by(country, invoice_no) %>%\n",
        "  summarise(order_value = sum(total_price), .groups = 'drop') %>%\n",
        "  group_by(country) %>%\n",
        "  summarise(avg_order_value_country = mean(order_value), .groups = 'drop')\n",
        "\n",
        "global_mean <- mean(aov_per_country$avg_order_value_country)\n",
        "global_sd <- sd(aov_per_country$avg_order_value_country)\n",
        "\n",
        "# Standardization based on global mean and sd\n",
        "aov_per_country <- aov_per_country %>%\n",
        "  mutate(aov_zscore = (avg_order_value_country - global_mean) / global_sd) %>%\n",
        "  arrange(desc(aov_zscore))\n",
        "\n",
        "\n",
        "head(aov_per_country)\n",
        "dim(aov_per_country)"
      ],
