        "global <- df %>%\n",
        "  count(category, sub_category, sort = TRUE) %>%\n",
        "  top_n(10) %>%\n",
        "  ggplot(aes(x = reorder(sub_category, n), y = n, fill = category)) +\n",
        "  geom_col() +\n",
        "  coord_flip() +\n",
        "  labs(title = \"Top Subcategories by Volume\", x = \"Sub Category\", y = \"Count\")\n",
        "\n",
