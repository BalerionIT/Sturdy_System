    {
      "cell_type": "code",
      "source": [
        "#CLV per country\n",
        "clv_per_country <- clv %>%\n",
        "  group_by(country) %>%\n",
        "  summarise(total_clv = sum(clv), .groups = \"drop\") %>% arrange(desc(total_clv))\n",
        "\n",
        "global_mean_clv <- mean(clv_per_country$total_clv)\n",
        "global_sd_clv <- sd(clv_per_country$total_clv)\n",
        "\n",
        "\n",
        "clv_per_country <- clv_per_country %>%\n",
        "  mutate(clv_zscore = (total_clv - global_mean_clv) / global_sd_clv)\n",
        "\n",
        "# Visualizzare i risultati\n",
        "head(clv_per_country, 10)\n",
        "dim(clv)"
      ],
