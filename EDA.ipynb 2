    {
      "cell_type": "code",
      "source": [
        "set.seed(100)\n",
        "model <- kmeans(rfm_kmeans, centers = 4, nstart = 30)\n",
        "rfm_kmeans$clusters <- model$cluster"
      ],
      "metadata": {
        "id": "v6qk2OkUXGBH"
      },
      "execution_count": 44,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "mean_recency <- mean(rfm_kmeans$recency_score)\n",
        "mean_frequency <- mean(rfm_kmeans$frequency_score)\n",
        "mean_monetary <- mean(rfm_kmeans$monetary_score)\n",
        "\n",
        "\n",
        "df_clusters <- rfm_kmeans %>%\n",
        "  group_by(clusters) %>%\n",
        "  summarise(recency_mean = mean(recency_score), frequency_mean = mean(frequency_score),\n",
        "    monetary_mean = mean(monetary_score))\n",
        "\n",
        "df_diff_from_mean <- df_clusters %>%\n",
        "  mutate(recency_diff = recency_mean - mean_recency, frequency_diff = frequency_mean - mean_frequency, monetary_diff = monetary_mean - mean_monetary)\n",
        "\n",
        "\n",
        "df_diff_table <- df_diff_from_mean %>%\n",
        "  select(clusters, recency_diff, frequency_diff, monetary_diff) %>%\n",
        "  pivot_longer(cols = -clusters, names_to = \"variable\", values_to = \"diff_from_mean\") %>%\n",
        "  pivot_wider(names_from = clusters, values_from = diff_from_mean)\n",
        "\n",
        "df_diff_table\n"
      ],
