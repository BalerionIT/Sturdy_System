            "text/latex": "A tibble: 1 × 3\n\\begin{tabular}{lll}\n total\\_items & total\\_orders & AIPO\\\\\n <dbl> & <int> & <dbl>\\\\\n\\hline\n\t 3693064 & 22088 & 167.1978\\\\\n\\end{tabular}\n",
            "text/plain": [
              "  total_items total_orders AIPO    \n",
              "1 3693064     22088        167.1978"
            ]
          },
          "metadata": {}
        }
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "#aipo per country\n",
        "\n",
        "aipo_per_country <-  df %>%\n",
        "  group_by(country) %>%\n",
        "  summarise(total_items = sum(quantity), total_orders = n_distinct(invoice_no)) %>%\n",
        "  mutate(AIPO = total_items / total_orders)\n",
        "\n",
        "global_mean_aipo <- mean(aipo_per_country$AIPO)\n",
        "global_sd_aipo <- sd(aipo_per_country$AIPO)\n",
        "\n",
        "\n",
        "aipo_per_country <- aipo_per_country %>%\n",
        "  mutate(aipo_zscore = (AIPO - global_mean_aipo) / global_sd_aipo) %>%\n",
        "  arrange(desc(aipo_zscore))\n",
        "\n",
        "head(aipo_per_country)"
      ],
      "metadata": {
        "colab": {
