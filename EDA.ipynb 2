      "source": [
        "Germany <- df %>% filter(country == 'Germany') %>%\n",
        "  count(category, sort = TRUE) %>%\n",
        "    top_n(10) %>% ggplot(aes(x =reorder(category, n), y = n, fill = as.factor(category))) +\n",
        "    geom_col()+\n",
        "      labs(title = 'Top Product in Germany', x = 'Category', y = 'Count')+\n",
        "      theme(axis.text.x = element_text(angle = 45, hjust =0))\n",
        "UK <- df %>% filter(country == 'United Kingdom') %>%\n",
        "  count(category, sort = TRUE) %>%\n",
        "    top_n(10) %>% ggplot(aes(x =reorder(category, n), y = n, fill = as.factor(category))) +\n",
        "    geom_col()+\n",
        "      labs(title = 'Top Product in United Kingdom', x = 'Category', y = 'Count')+\n",
        "       theme(axis.text.x = element_text(angle = 45, hjust =0))\n",
        "\n",
        "grid.arrange(Germany, UK, nrow=1)\n"
